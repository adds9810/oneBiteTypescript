{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "dist", // 컴파일 한 파일 저장 경로
    "moduleDetection": "force", // 엄격한 null 검사
    "strictNullChecks": false,
    "allowJs": true // 자바스크립트 파일 허용 여부
    // "noImplicitAny": true
  },
  "include": ["src"] // 컴파일 저장 경로
}
